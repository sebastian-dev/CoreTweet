#root Labs.V2
#namespace HideRepliesApi
#urlprefix "labs/2"
#urlsuffix ""
#description Provides a set of methods for the wrapper of Hide replies v2 API on Labs v2.

endpoint HideRepliesResponse Hidden : Put tweets/{id}/hidden
{
    description
    {
        Hides a Tweet.
    }
    returns
    {
        The data that indicates if the Tweet was successfully hidden.
    }
    jsonmap
    {
        "hidden": $hidden
    }
    params
    {
        required long id
        required bool hidden
    }
}
